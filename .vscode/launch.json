    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'mysqltest'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=mysqltest",
                    "--package=mysqltest"
                ],
                "filter": {
                    "name": "mysqltest",
                    "kind": "bin"
                }
            },
            "args": ["--start-filename=mysql-bin.000001", "-j4"],
            // "args": ["-pbaikai#1234", "-P3306", "--start-filename=mysql-bin.000004", "-j4"],
            // "args": ["-Hgzdsg.baikai.top", "-uroot", "-p123456", "-P4407", "--start-filename=mysql-bin.000001", "-j4"],
            // "args": ["--start-filename=mysql-bin.000002", "-j4"],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'mysqltest'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=mysqltest",
                    "--package=mysqltest"
                ],
                "filter": {
                    "name": "mysqltest",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
